#include<stdio.h>
int main()
{
  int n,i;
  int temp;
  int flag=0;
  printf("Enter the number\n");
  scanf("%d",&n);
  for(i=2;i<=n;i++)
  {
      temp =n%i;
      if(temp==0)
      {
        flag=1;
      }
  }
    if(n==1)
  {
    printf("Prime (or) Compsite");
  }
   
    else
    if(flag==1)
        {
            printf("Not a prime number\n");
        }
      else
      {
        printf("Prime number\n");
      }
    
}
